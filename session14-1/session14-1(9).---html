<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <script>
        var a = new Array();
        var i;
        var j;
        for(i = 0; i < 3; i++){
            for(j = 0; j < 3; j++){
                do{
                    a[i][j] = parseInt(prompt("Nhập vào số nghuyên dương bất kì: " + "[" + i + "]" + "[" + j + "]"));
                } while(a[i][j] < 0);
                document.write(a[i][j]);
            }
        }
    </script>
</body>
</html>